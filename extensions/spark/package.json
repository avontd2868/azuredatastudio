        "@types/node": "^7.0.43",
        "@types/react": "^16.4.13",
        "@types/react-dom": "^16.0.7",
        "@types/request": "^2.48.1",
        "@types/sanitize-html": "^1.18.0",
        "@types/which": "^1.3.1",
        "ansi_up": "^3.0.0",
        "child-process-promise": "^2.2.1",
        "copy-webpack-plugin": "^4.5.1",
        "css-loader": "^1.0.0",
